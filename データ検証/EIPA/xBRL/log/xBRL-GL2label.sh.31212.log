# === tsv -> xml ============================================================
# seq parent code level type module term description
# 1   2      3    4     5    6      7    8
# see https://stackoverflow.com/questions/8024392/awk-replace-a-column-with-its-hash-value
cat gl/source/xBRL-GL.txt | awk -F'\t' -v module=$1 'BEGIN {
  tmp="echo " $3 " | openssl md5 | cut -f2 -d\" \"";
  tmp | getline cksum;
  close(tmp);
  print cksum "\t" $0;
}' > $Tmp-cksum
